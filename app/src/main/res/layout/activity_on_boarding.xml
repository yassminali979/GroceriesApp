<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="855dp"
    android:background="@drawable/onboarding">

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="38.21dp"
        android:layout_height="43.53dp"
        android:background="@drawable/carrot"
        android:viewportWidth="38.21"
        android:viewportHeight="43.53"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.471">

    </ImageView>

    <TextView
        android:id="@+id/imageView2"
        android:layout_width="230dp"
        android:layout_height="77dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/welcome"
        android:gravity="center_horizontal|top"
        android:lineSpacingExtra="-30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:layout_constraintVertical_bias="0.072">

    </TextView>

    <TextView
        android:id="@+id/imageView3"
        android:layout_width="270dp"
        android:layout_height="15dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/getwelcom"
        android:gravity="center_horizontal|top"
        android:lineSpacingExtra="-4sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView2"
        app:layout_constraintVertical_bias="0.052">

    </TextView>

    <Button
        android:id="@+id/get_started"
        android:layout_width="300dp"
        android:layout_height="55dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentRight="true"
        android:background="@drawable/shape"
        android:text="@string/get_started"
        android:textAppearance="@style/get_started"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:layout_constraintVertical_bias="0.17">
    </Button>
</androidx.constraintlayout.widget.ConstraintLayout>
